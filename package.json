{
  "name": "@barbapapazes/plausible-tracker",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@9.0.6+sha256.0624e30eff866cdeb363b15061bdb7fd9425b17bc1bb42c22f5f4efdea21f6b3",
  "description": "Fork of frontend tracker to interact with Plausible Analytics",
  "license": "MIT",
  "repository": "https://github.com/barbapapazes/plausible-tracker",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./extensions/*": {
      "import": {
        "types": "./dist/extensions/*.d.ts",
        "default": "./dist/extensions/*.mjs"
      },
      "require": {
        "types": "./dist/extensions/*.d.ts",
        "default": "./dist/extensions/*.cjs"
      }
    },
    "./extensions": {
      "import": {
        "types": "./dist/extensions/index.d.ts",
        "default": "./dist/extensions/index.mjs"
      },
      "require": {
        "types": "./dist/extensions/index.d.ts",
        "default": "./dist/extensions/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp",
    "test": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.0",
    "@types/node": "^18.19.31",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "happy-dom": "^13.10.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.2"
  }
}
